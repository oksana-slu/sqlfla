var adjustWidth;

adjustWidth = function() {
  var addonWidth, inputWidth;
  _($(".input-append")).each(function(a, b) {
    return console.log(a, b);
  });
  inputWidth = parseInt($(".input-append input").css('width'));
  addonWidth = parseInt($(".input-append .add-on").css('width'));
  return console.log(inputWidth, addonWidth);
};

$(function() {
  return adjustWidth();
});
